.body {
  box-sizing: border-box;
  padding: 0 15px;
  border-radius: 20rpx;
  background-color: #fff;
  margin-bottom: 30rpx;
}
.list-area {
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #eee;
  box-sizing: border-box;
}
.list-area:last-child {
  border-bottom: none;
}